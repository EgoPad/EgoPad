<%= semantic_form_for @note do |f| %>
  <%= f.inputs do %>
        <%= f.input :title, :label => "Название" %>
        <%= f.input :url, :label => "Ссылка" %>
        <%= f.input :sharable, :label => "Общий доступ" %>
        <%= f.input :content, :label => "Содержимое" %>
        <%= f.input :ncategory, :label => "Категория" %>
        <%= f.input :tag_list %>
  <% end %>
  <%= f.buttons %>
<% end %>
